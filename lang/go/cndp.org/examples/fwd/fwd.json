{
    "application": {
        "name": "fwd",
        "description": "Simple packet forward examples using Go CNDP bindings"
    },

    "defaults": {
        "bufcnt": 16,
        "bufsz": 2,
        "rxdesc": 2,
        "txdesc": 2,
        "cache": 256,
        "mtype": "2MB"
    },

    "umems": {
        "umem0": {
            "bufcnt": 32,
            "bufsz": 2,
            "mtype": "2MB",
            "regions": [
                16,
                16
            ],
            "rxdesc": 0,
            "txdesc": 0,
            "description": "UMEM Description 0"
        }
    },

    "lports": {
        "enp25s0f0:0": {
            "pmd": "net_af_xdp",
            "qid": 36,
            "umem": "umem0",
            "region": 0,
            "description": "LAN 0 port"
        },
        "enp25s0f1:0": {
            "pmd": "net_af_xdp",
            "qid": 37,
            "umem": "umem0",
            "region": 1,
            "description": "LAN 1 port"
        }
    },

    "lcore-groups": {
        "initial": [10],
        "group0": [13],
        "group1": [14],
        "default": ["15-16"]
    },

    "options": {
        "pkt_api": "pktdev",
        "no-metrics": false,
        "no-restapi": false,
        "cli": true,
        "mode": "drop"
    }
}
