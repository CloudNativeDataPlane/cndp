+impl<'a> Drop for CndpUdpReader<'a> {
+    fn drop(&mut self) {
+        log::debug!("CndpUdpReader drop");
+        let cne_uids = self.cne_uids.write().unwrap();
+        CndpUdpCommon::unregister_cne_uids(&(*cne_uids));
+    }
+}
