+                pktdev_buf_alloc(
+                    fport.lport as i32,
+                    tx_pktmbufs_pptr,
+                    num_pkts_to_send as u16,
+                ) as u16
+            };
+            if n_pkts < num_pkts_to_send as u16 {
+                log::debug!("Cannot allocate enough buffers to send all packets");
+                log::debug!("Requested = {}, Allocated = {}", num_pkts_to_send, n_pkts);
+            }
+            if n_pkts > 0 {
+                log::debug!(
+                    "Send {} UDP packets at time {}",
